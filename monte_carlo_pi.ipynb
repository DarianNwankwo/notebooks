   "source": [
    "function monte_carlo_pi(nsamples)\n",
    "    acc = 0\n",
    "    for i in 1:nsamples\n",
    "        x = rand()\n",
    "        y = rand()\n",
    "        if (x^2 + y^2) < 1.0\n",
    "            acc += 1\n",
    "        end\n",
    "    end\n",
    "    return 4.0 * acc / nsamples\n",
    "end"
   ]
